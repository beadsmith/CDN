@-webkit-keyframes myAnimation {
    0% {
        opacity: 0;
        top: -20%;
        left: 0;
    }
    50% {
        opacity: 1;
        top: 50%;
        left: 0;
    }
    100% {
        opacity: 0;
        top: 90%;
        left: 0;
    }
}
@-moz-keyframes myAnimation {
    0% {
        opacity: 0;
        top: -20%;
        left: 0;
    }
    50% {
        opacity: 1;
        top: 50%;
        left: 0;
    }
    100% {
        opacity: 0;
        top: 90%;
        left: 0;
    }
}
@-o-keyframes myAnimation {
    0% {
        opacity: 0;
        top: -20%;
        left: 0;
    }
    50% {
        opacity: 1;
        top: 50%;
        left: 0;
    }
    100% {
        opacity: 0;
        top: 90%;
        left: 0;
    }
}
@keyframes myAnimation {
    0% {
        opacity: 0;
        top: -20%;
        left: 0;
    }
    50% {
        opacity: 1;
        top: 50%;
        left: 0;
    }
    100% {
        opacity: 0;
        top: 90%;
        left: 0;
    }
}
